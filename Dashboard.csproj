        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>aspnet-Climate_Watch-CD5D5E04-A514-4323-9B43-4F641FDBAF99</UserSecretsId>

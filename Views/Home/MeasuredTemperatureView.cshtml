@model IEnumerable<Climate_Watch.Models.TemperatureModel>
@{
    ViewData["Title"] = "Measured Temperature";
}

<h2>Measured Temperature</h2>
<h1></h1>
<table class="table" style="margin-top: 10px">
    <thead>
    <tr>
        <th>Created_at</th>
        <th>Start_at</th>
        <th>End_at</th>
        <th>Device</th>
        <th>Measurement</th>
        <th>Value</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Created_at</td>
            <td>@item.Start_at</td>
            <td>@item.End_at</td>
            <td>@item.Device</td>
            <td>@item.Measurement</td>
            <td>@item.Value</td>
        </tr>
    }
    </tbody>
</table>


<div id="sensorModal" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Sensors</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="sensorModalBody">
            </div>
        </div>
    </div>
</div>